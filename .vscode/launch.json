    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/tracer.exe",
            "args": ["pyrs", "pyrsopts", "640", "400"],
            "symbolSearchPath": "",
            "console": true,
            "logging": {
              "moduleLoad": false,
              "trace": true
            },
          }    ]
}